# Server
PORT=3001

# Environment: "development" | "production"
NODE_ENV=development

# EVM private key — same key as authorizedEVMAddress in CRE config
EVM_PRIVATE_KEY=0x...

# Shared secret for CRE → backend settlement callback (X-CRE-Secret header)
CRE_CALLBACK_SECRET=your_secret_here

# Infura API Key
INFURA_API_KEY=

# World ID Registry
WORLD_ID_REGISTRY=0xb1eA4506e10e4Be8159ABcC7A7a67C614a13A425

# ── Production-only (NODE_ENV=production) ──
# CRE gateway URL (default: https://01.gateway.zone-a.cre.chain.link)
# CRE_GATEWAY_URL=https://01.gateway.zone-a.cre.chain.link
# 64-char hex workflow ID (no 0x prefix) from `cre workflow deploy` output
# CRE_WORKFLOW_ID=

# Finnhub API Key
FINNHUB_API_KEY=

# OpenAI API Key
OPENAI_API_KEY=